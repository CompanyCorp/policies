{
  "tasks": {
    "dev": "deno run -A --node-modules-dir=auto npm:vite",
    "build": "deno run -A --node-modules-dir=auto npm:vite build",
    "server:start": "deno run -A --node-modules-dir --watch ./server/main.ts",
    "serve": "deno task build && deno task server:start"
  },
  "imports": {
    "@deno/vite-plugin": "npm:@deno/vite-plugin@^1.0.0",
    "@types/react": "npm:@types/react@^18.3.3",
    "@types/react-dom": "npm:@types/react-dom@^18.3.3",
    "@vitejs/plugin-react": "npm:@vitejs/plugin-react@^4.3.4",
    "react": "npm:react@^18.3.1",
    "react-dom": "npm:react-dom@^18.3.1",
    "vite": "npm:vite@^6.0.5",
    "@emotion/react": "npm:@emotion/react@11.14.0",
    "@emotion/styled": "npm:@emotion/styled@11.14.0",
    "@fontsource/nunito": "npm:@fontsource/nunito@5.1.1",
    "@mui/material": "npm:@mui/material@6.4.0",
    "@mui/x-tree-view": "npm:@mui/x-tree-view@7.24.0",
    "@discretize/gw2-ui-new": "npm:@discretize/gw2-ui-new@3.1.0",
    "@discretize/typeface-menomonia": "npm:@discretize/typeface-menomonia@0.1.3",
    "wouter": "npm:wouter@^3.3.5",
    "@gw2": "./src/gw2/index.ts",
    "@gw2/*": "./src/gw2/*"
  },
  "lint": {
    "exclude": [
      "node_modules",
      ".vite"
    ]
  },
  "compilerOptions": {
    "types": [
      "react",
      "react-dom",
      "npm:@types/react",
      "npm:@types/react-dom"
    ],
    "lib": [
      "dom",
      "dom.iterable",
      "deno.ns"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
